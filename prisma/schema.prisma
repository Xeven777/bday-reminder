generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model BdateInfo {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  bdate       String
  name        String
  tag         String
  userId      String
  friendEmail String?
  autosend    Boolean?
  createdAt   DateTime @default(now())

  @@map("bdayInfo")
}
